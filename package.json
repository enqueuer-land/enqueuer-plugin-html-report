{
  "name": "enqueuer-plugin-html-report",
  "version": "1.0.0",
  "description": "Enqueuer html-report plugin",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "all": "npm run clean && npm run lint && npm run build && npm run updateIndex && npm run validateOutput",
    "build": "tsc",
    "clean": "rm -rf dist",
    "updateIndex": "nqr -c conf.yml -l `pwd` && mv output.html index.html",
    "validateOutput": "node_modules/.bin/html5v index.html",
    "lint": "node_modules/.bin/tslint --project tsconfig.json",
    "prepublishOnly": "npm run all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/enqueuer-land/enqueuer-plugin-html-report.git"
  },
  "keywords": [
    "enqueuer",
    "nqr",
    "html",
    "report",
    "plugin"
  ],
  "author": "Virgs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/enqueuer-land/enqueuer-plugin-html-report/issues"
  },
  "homepage": "https://github.com/enqueuer-land/enqueuer-plugin-html-report#readme",
  "devDependencies": {
    "@types/node": "^11.13.15",
    "html5-validator": "^1.2.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "enqueuer": "^5.0.0"
  }
}
